    "scripts": {
        "build": "next build",
        "build:e2e": "NEXT_PUBLIC_ASSET_PREFIX= NEXT_PUBLIC_DIGISOS_ENV=e2e pnpm build && cp -R .next/static .next/standalone/.next",
        "tsc": "tsc",
        "dev": "next dev -p 3001 -H 0.0.0.0 | pino-pretty --messageKey=message",
        "start": "next start -H 0.0.0.0",
        "test": "vitest",
        "e2e": "playwright test",
        "e2e:ui": "playwright test --ui",
        "e2e:headed": "playwright test --headed",
        "prepare": "husky",
        "checkUpdates": "npx npm-check-updates -u -c 7",
        "fetch-old-api": "curl -o ./soknad-api-old.json http://localhost:8181/sosialhjelp/soknad-api/v3/api-docs/old && npx --yes openapi-format soknad-api-old.json -o soknad-api-old.json",
        "fetch-new-api": "curl -o ./soknad-api-new.json http://localhost:8181/sosialhjelp/soknad-api/v3/api-docs/new && npx --yes openapi-format soknad-api-new.json -o soknad-api-new.json",
        "fetch-api": "pnpm run fetch-old-api && pnpm run fetch-new-api",
        "orval": "rm -rf ./src/generated && orval --prettier",
        "lint": "eslint",
        "preinstall": "npx -y only-allow pnpm"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{less,css,md}": "prettier --write",
        "*.{ts,tsx}": [
            "prettier --write",
            "eslint --fix --max-warnings 1"
        ]
    }
